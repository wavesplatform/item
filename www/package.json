{
  "name": "item-protocol",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p start:*",
    "start:item-protocol": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "run-s lint:* && ng lint",
    "lint:fix": "ng lint --fix",
    "lint:prettier-ts": "prettier --ignore-path .node_modules --check **/*.ts --write",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "serve:ssr:item-protocol": "PUBLIC_URL=https://item.sh/ node dist/server",
    "build:ssr": "run-p build:ssr:*",
    "build:ssr:item-protocol": "npm run build:client-and-server-bundles:item-protocol && npm run compile:server",
    "build:client-and-server-bundles:item-protocol": "ng build item-protocol && ng run item-protocol:server:production --bundleDependencies all",
    "docker": "run-s docker:*",
    "docker:build": "docker build -t landing .",
    "docker:serve": "docker run -p 3000:3000 landing",
    "icons": "npx icon-font-generator ./resources/icons/*.svg -o ./resources/assets/fonts/icons --csspath=\"resources/styles/_icons.scss\" --csstp=\"./resources/icons/scss.hbs\" --html=false --name=icomoon --json=false --height=1024 --descent=64 --round=10e8 --ts=1552345600"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/platform-server": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@w11k/ngx-componentdestroyed": "^4.1.4",
    "cli-progress": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "express-http-proxy": "^1.6.0",
    "icon-font-generator": "git+https://github.com/ln-e/icon-font-generator.git",
    "include-media": "^1.4.9",
    "luxon": "^1.16.0",
    "ng-yandex-metrika": "^3.0.2",
    "ngx-captcha": "^7.0.0",
    "ngx-date-countdown": "^1.1.3",
    "ngx-swiper-wrapper": "^8.0.2",
    "node-cache": "^5.0.1",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.4.0",
    "rxjs-tslint-rules": "^4.26.1",
    "susy": "^3.0.5",
    "tslib": "^1.10.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "zone.js": "~0.9.1"
  },
