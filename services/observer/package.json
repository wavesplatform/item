  },
  "devDependencies": {
    "@item-protocol/types": "^1.1.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
